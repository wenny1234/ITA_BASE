<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<!-- ログに出力する際の、 ログのカテゴリー名の先頭に付与する。 -->
		<setting name="logPrefix" value="sql."/>
	</settings>
	
    <plugins>
        <plugin interceptor="jp.co.tokyo_gas.cisfw.sql.register.CfwSQLRegister" />
    </plugins>
	
	<environments default="db2">
		<environment id="db2">
			<transactionManager type="MANAGED" />
			<dataSource type="POOLED">
				<property name="driver" value="com.ibm.db2.jcc.DB2Driver" />
				<property name="url" value="jdbc:db2://localhost:50000/CIRI1170" />
				<property name="username" value="cirigt01" />
				<property name="password" value="cirigt01" />
			</dataSource>
		</environment>
	</environments>
	    
    <mappers>
